{
  "projects": {
    "default": "" // CHANGE WITH YOUR FIREBASE PROJECT ID
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "__pycache__",
        ".mypy_cache",
        ".pytest_cache",
        "/functions/venv",
        "/functions/.mypy_cache",
        "/functions/.pytest_cache",
        "/functions/functions/__pycache__",
        "/functions/functions/**/__pycache__",
        "/functions/functions/**/**/__pycache__",
        "/functions/functions/**/**/**/__pycache__",
        "/functions/functions/**/**/*/__pycache__"
      ]
    }
  ],
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
