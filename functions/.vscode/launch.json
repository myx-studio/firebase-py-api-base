{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/functions",
            "python": "${workspaceFolder}/functions/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/functions"
            }
        }
    ]
}
